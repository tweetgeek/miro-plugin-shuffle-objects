{
  "name": "miro-plugin-shuffle-objects",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "NODE_ENV=production webpack",
    "watch": "webpack -w",
    "prettier": "prettier --check ./",
    "lint": "eslint ./",
    "deploy": "yarn install && yarn prebuild && yarn build && gh-pages -d dist"
  },
  "license": "MIT",
  "devDependencies": {
    "@mirohq/prettier-config": "^1.0.0",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "css-loader": "4.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "style-loader": "1.2.1",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "8.0.3",
    "typescript": "4.0.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,svg,json,md,ts,js,tsx}": [
      "prettier --write"
    ],
    "*.{ts,js,tsx}": [
      "eslint"
    ]
  },
  "browserslist": [
    ">0.25%",
    "not op_mini all",
    "not ie <= 11"
  ]
}
